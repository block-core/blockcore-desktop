{
  "name": "blockcorehub",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Blockcore",
    "email": "post@blockcore.net",
    "url": "https://www.blockcore.net"
  },
  "description": "Blockcore Hub is your hub into everything on blockchains and the Smart City Platform.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/block-core/blockcore-hub.git"
  },
  "bugs": {
    "url": "https://github.com/block-core/blockcore-hub/issues"
  },
  "homepage": "https://github.com/block-core/blockcore-hub#readme",
  "main": "main.js",
  "scripts": {
    "prepare": "node -e \"console.log('APP_VERSION = \\'' + process.env.npm_package_version +'\\';')\" > src/version.js",
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "start": "npm run prepare && npm run postinstall:electron && npm run electron:tsc && npm-run-all -p ng:serve electron:serve",
    "ng": "ng",
    "build": "npm run postinstall:electron && npm run electron:tsc && ng lint && ng build",
    "build:prod": "npm run build -- -c production",
    "ng:lint": "ng lint",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:tsc": "tsc main.ts --lib DOM,ES2018",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:testnet && electron .",
    "builder": "npm run prepare && npm run build:prod && npx electron-builder build",
    "build:linux": "npm run builder -- --linux",
    "build:linuxsnap": "npm run builder -- --linux snap",
    "build:win:x86": "export arch=x86|| set arch=x86&& npm run builder -- --windows --ia32",
    "build:win": "export arch=x64|| set arch=x64&& npm run builder -- --windows --x64",
    "build:osx": "npm run builder -- --mac",
    "build:all": "npm run builder -- -mwl",
    "publish": "npm run builder --  -wl -p always",
    "publish:osx": "npm run builder --  -m -p always",
    "publish:win:x86": "export arch=x86|| set arch=x86&& npm run builder --  -w --ia32 -p always",
    "publish:win": "export arch=x64|| set arch=x64&& npm run builder --  -w  --x64 -p always",
    "publish:linux": "npm run builder --  -l -p always",
    "publish:linuxsnap": "npm run builder --  -l snap -p always",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e",
    "version": "node -p \"require('./package.json').version\"",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.1",
    "@angular/cdk": "^13.3.2",
    "@angular/common": "~13.3.1",
    "@angular/compiler": "~13.3.1",
    "@angular/core": "~13.3.1",
    "@angular/flex-layout": "13.0.0-beta.38",
    "@angular/forms": "~13.3.1",
    "@angular/language-service": "13.3.1",
    "@angular/material": "^13.3.2",
    "@angular/platform-browser": "~13.3.1",
    "@angular/platform-browser-dynamic": "~13.3.1",
    "@angular/router": "~13.3.1",
    "@blockcore/message": "^1.0.0",
    "@electron/remote": "^2.0.8",
    "@fontsource/roboto": "^4.5.5",
    "@types/qrcode": "^1.4.2",
    "base64url": "^3.0.1",
    "conventional-changelog-cli": "2.2.2",
    "electron-context-menu": "^3.1.2",
    "electron-log": "^4.4.6",
    "electron-updater": "^4.6.5",
    "extract-zip": "^2.0.1",
    "lodash": "^4.17.21",
    "material-icons": "^1.10.8",
    "ngx-image-cropper": "^6.1.0",
    "ngx-material-file-input": "3.0.0",
    "qrcode": "^1.5.0",
    "stream-browserify": "^3.0.0",
    "tslib": "^2.3.1",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.1",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "13.3.1",
    "@angular/compiler-cli": "13.3.1",
    "@electron/typescript-definitions": "^8.9.6",
    "@types/jasmine": "~4.0.2",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "city-coininfo": "5.0.0",
    "core-js": "3.21.1",
    "electron": "^18.0.1",
    "electron-builder": "^22.14.13",
    "electron-reload": "1.5.0",
    "enhanced-resolve": "5.9.2",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "38.1.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "hammerjs": "2.0.8",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "npm-run-all": "4.1.5",
    "npx": "^10.2.2",
    "protractor": "~7.0.0",
    "replace": "1.2.1",
    "rxjs": "7.5.5",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "wait-on": "6.0.1",
    "webpack-cli": "^4.9.2",
    "webpack-ext-reloader": "1.1.9",
    "zone.js": "~0.11.5"
  }
}
